
  <div class="row">
    <div class="col s12 m3">
      {{> adminSidebar}}
    </div>
    <div class="col s12 m9">
      <h5>Завершенные заказы</h5>

      {{#each orders}}
        <div class="card">
          <div class="card-content">
            <span class="card-title">Заказ №{{id}} (Статус: {{status}})</span>
            <p><b>Имя:</b> {{full_name}}</p>
            <p><b>Телефон:</b> {{phone}}</p>
            <p><b>Адрес:</b> {{address}}</p>
            <p><b>Дата создания:</b> {{created_at}}</p>
          </div>
        </div>
      {{else}}
        <p>Нет завершенных заказов</p>
      {{/each}}
    </div>
  </div>
